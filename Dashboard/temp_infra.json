        "versioning": false
      }
    },
    {
      "id": "sg-db-production",
      "type": "aws_security_group",
      "status": "high",
      "config": {
        "inbound_rules": [
          {
            "port": 22,
            "source": "0.0.0.0/0",
            "description": "SSH aberto para o mundo"
          },
          {
            "port": 3389,
            "source": "0.0.0.0/0",
            "description": "RDP aberto"
          }
        ]
      }
    }
  ]
}